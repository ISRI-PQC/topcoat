    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "name": "POC local",
            "type": "python",
            "request": "launch",
            "program": "src/run_locally.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "CLI keygen",
            "type": "python",
            "request": "launch",
            "program": "src/run_cli.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["-c", "config_template.yaml", "keygen", "-o", "key.pkl"]
        }
    ]
}